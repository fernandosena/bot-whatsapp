# Configurações do Bot
HEADLESS=true

# Configurações de busca padrão
DEFAULT_SECTOR=lanchonetes
DEFAULT_CITY=São Paulo

# Configurações do banco de dados
DB_PATH=./database/empresas.db

# Tempo de espera (segundos) - Otimizado para performance
WAIT_TIME=1
PAGE_LOAD_TIMEOUT=20

# Performance e Limites
# CHUNK_SIZE: Quantas URLs processar por vez (evita sobrecarga de memória)
# Recomendado: 50-100 para sistemas normais, 200-500 para servidores
CHUNK_SIZE=100

# MAX_RESULTS_PER_SEARCH: Máximo de resultados por busca
# Google Maps geralmente mostra até 120 resultados por busca
# Recomendado: 50-120
MAX_RESULTS_PER_SEARCH=120

# BATCH_COMMIT: Quantas empresas salvar antes de fazer commit no banco
# Valores maiores = mais rápido, mas mais arriscado se houver erro
# Recomendado: 10-50
BATCH_COMMIT=20
